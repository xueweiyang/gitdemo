<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tencent.matrix.resource.test" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <instrumentation
        android:name="android.test.InstrumentationTestRunner"
        android:functionalTest="false"
        android:handleProfiling="false"
        android:label="Tests for com.tencent.matrix.resource.test"
        android:targetPackage="com.tencent.matrix.resource.test" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application android:debuggable="true" >
        <uses-library android:name="android.test.runner" />

        <service
            android:name="com.tencent.matrix.resource.CanaryWorkerService"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE"
            android:process=":res_can_worker" >
        </service>
        <service
            android:name="com.tencent.matrix.resource.CanaryResultService"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" >
        </service>
    </application>

</manifest>